{
  "name": "create-react-redux-app-structure",
  "version": "1.1.3",
  "main": "index.js",
  "repository": "https://github.com/shystruk/create-react-redux-app-structure.git",
  "description": "create react redux app structure with build configurations",
  "keywords": [
    "react",
    "redux",
    "react + redux",
    "react redux folder structure",
    "react folder structure"
  ],
  "author": "shystruk",
  "license": "MIT",
  "scripts": {
    "server": "nodemon --delay 2 index.js",
    "dev": "concurrently \"webpack --watch\" \"gulp\"",
    "prod": "gulp clean-build && grunt remove-code && webpack && gulp prod --production && grunt",
    "staging": "gulp clean-build && webpack && gulp prod --production && grunt",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "report-coverage": "cat ./app/tests/__tests__/coverage/lcov.info | codecov",
    "e2e": "run-p --race dev server cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec ./app/tests/e2e/cypress/integration/create_react_redux_app_structure_spec.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "jest": {
    "verbose": true,
    "globals": {},
    "testPathIgnorePatterns": [
      "__snapshots__",
      "coverage",
      "configurations"
    ],
    "coverageDirectory": "app/tests/__tests__/coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/*.jsx",
      "app/*.js",
      "app/*/*.jsx",
      "app/*/*.js",
      "app/*/*/*.jsx",
      "app/*/*/*.js",
      "!app/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 2,
        "functions": 2,
        "lines": 2,
        "statements": 2
      }
    }
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.25.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov.io": "^0.1.6",
    "concurrently": "^3.5.0",
    "cypress": "^1.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "ghooks": "^2.0.0",
    "grunt": "^1.0.1",
    "grunt-cache-busting": "^0.0.11",
    "grunt-cli": "^1.2.0",
    "grunt-replace": "^1.0.1",
    "grunt-strip-code": "^1.0.6",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.4.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.0.0",
    "webpack-merge": "^4.1.0"
  }
}
